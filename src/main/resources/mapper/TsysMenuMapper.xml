<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 系统菜单表(TSYS_MENU) -->
<mapper namespace="cn.tablego.project.springboot.mapper.TsysMenuMapper">
    <!-- 字段映射 -->
    <resultMap id="tsysMenuMap" type="cn.tablego.project.springboot.model.TsysMenu"/>

    <!-- This code was generated by TableGo tools, mark 1 begin. -->
    <!-- 表所有字段 -->
    <sql id="allColumns">
        tm.MENU_CODE, tm.KIND_CODE, tm.TRANS_CODE, tm.SUB_TRANS_CODE, tm.MENU_NAME, tm.MENU_ARG, tm.MENU_ICON, tm.MENU_URL, 
        tm.WINDOW_TYPE, tm.WINDOW_MODEL, tm.TIP, tm.HOT_KEY, tm.PARENT_CODE, tm.ORDER_NO, tm.OPEN_FLAG, tm.TREE_IDX, 
        tm.REMARK
    </sql>
    <!-- This code was generated by TableGo tools, mark 1 end. -->

    <!-- 根据条件分页查询系统菜单表列表 -->
    <select id="findTsysMenuPage" resultMap="tsysMenuMap">
        SELECT
            <include refid="allColumns" />
        FROM TSYS_MENU tm WHERE 1 = 1
    </select>

</mapper>